{
  "name": "@hanzo/matching-engine",
  "version": "1.0.1",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hanzoai/matching-engine"
  },
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-decorators": "7.6.0",
    "@babel/plugin-proposal-object-rest-spread": "7.6.2",
    "@babel/preset-env": "7.6.2",
    "@babel/preset-typescript": "7.7.2",
    "@types/jest": "24.0.22",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.9.0",
    "concurrently": "4.1.2",
    "jest": "24.9.0",
    "node-eval": "2.0.0",
    "nodemon": "1.19.4",
    "rollup": "1.23.0",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-filesize": "6.2.1",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-peer-deps-external": "2.2.0",
    "rollup-plugin-visualizer": "2.7.2",
    "serve": "11.2.0",
    "source-map-explorer": "2.1.0",
    "source-map-support": "0.5.16",
    "ts-jest": "24.1.0",
    "ts-node": "8.5.0",
    "typedoc": "github:TypeStrong/typedoc#5704bf5",
    "typedoc-plugin-markdown": "2.2.11",
    "typescript": "3.7.2"
  },
  "scripts": {
    "audit": "node_modules/.bin/source-map-explorer dist/index.js",
    "prepublishOnly": "npm run build",
    "clean:js": "rm -rf dist",
    "clean:docs": "rm -rf docs",
    "build": "npm run clean:js && npm run build:js && npm run build:types && npm run build:docs",
    "build:js": "npm run clean:js && node_modules/.bin/rollup -c",
    "build:docs": "npm run clean:docs && node_modules/.bin/typedoc --out docs --exclude '**/*+(test).ts' src",
    "build:types": "tsc --emitDeclarationOnly",
    "build:watch": "node_modules/.bin/rollup -c -w",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "watch": "node_modules/.bin/concurrently 'node_modules/.bin/nodemon src/server.ts --exec \"ts-node\" --watch ./src' 'node_modules/.bin/serve -l tcp://127.0.0.1'",
    "test": "node_modules/.bin/jest",
    "test:coverage": "node_modules/.bin/jest --collect-coverage",
    "serve": "ts-node src/server.ts"
  },
  "renovate": {
    "extends": [
      "config:js-lib"
    ],
    "automerge": true,
    "major": {
      "automerge": false
    },
    "automergeType": "branch"
  },
  "directories": {
    "src": "src",
    "dist": "dist"
  },
  "files": [
    "src",
    "dist"
  ],
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.4.4",
    "@babel/plugin-proposal-optional-chaining": "7.6.0",
    "@types/cors": "2.8.6",
    "@types/express": "4.17.2",
    "@types/moment-timezone": "0.5.12",
    "@types/node": "12.12.7",
    "@types/raf": "3.4.0",
    "@types/random": "2.1.0",
    "@types/socket.io": "2.1.4",
    "@types/uuid": "3.4.6",
    "avl": "1.4.4",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "d3-format": "1.4.2",
    "decimal.js": "10.2.0",
    "express": "4.19.2",
    "fast-memoize": "2.5.1",
    "http": "0.0.0",
    "mnemonist": "0.30.0",
    "moment": "2.24.0",
    "moment-timezone": "0.5.27",
    "raf": "3.4.1",
    "random": "2.1.1",
    "socket.io": "^2.3.0",
    "uuid": "3.3.3"
  },
  "peerDependencies": {
    "socket.io": "2.3.0"
  }
}
